{{= options.fileHeader }}

{{ for (const groupName of Object.keys(structure)) { }}
  {{ for (const itemName of Object.keys(structure[groupName])) { }}
    {{ const item = structure[groupName][itemName]; }}

    {{ if (options.type_enabledTypes.indexOf(item.type) === -1) { }}
    {{ continue; }}
    {{ } }}

    {{ if (options.useTypescript) { }}
      {{ const normalVariant = typeExec({ type: item.type, model: item, isInputType: false }).trim(); }}
      {{ const inputVariant = typeExec({ type: item.type, model: item, isInputType: true }).trim(); }}

      /**
       * {{= item.uniqueName }}
       * {{ if (item.docString) { }}Docs: {{= item.docString }}{{ } }}
       */
       export type {{= item.uniqueName }} = {{= normalVariant }};

      /**
       * {{= item.uniqueName }}_Input
       * {{ if (item.docString) { }}Docs: {{= item.docString }}{{ } }}
       */
       export type {{= item.uniqueName }}_Input = {{= normalVariant === inputVariant ? item.uniqueName : inputVariant }};

    {{ } else { }}
      {{ const normalVariant = typeExec({ type: item.type, model: item, isInputType: false }).trim(); }}
      {{ const inputVariant = typeExec({ type: item.type, model: item, isInputType: true }).trim(); }}

      /**
       * @name {{= item.uniqueName }}
       * {{ if (item.docString) { }}Docs: {{= item.docString }}{{ } }}
       * @typedef { {{= normalVariant }}}
       */

      /**
       * @name {{= item.uniqueName }}_Input
       * {{ if (item.docString) { }}Docs: {{= item.docString }}{{ } }}
       * @typedef { {{= normalVariant === inputVariant ? item.uniqueName : inputVariant }}}
       */

    {{ } }}
  {{ } }}
{{ } }}


